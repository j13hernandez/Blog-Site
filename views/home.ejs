<%-include('partials/header') -%>
<body>
  <div class="container">
    <h1>Homer</h1>
    <p><%=startingContent%></p>
  </div>
  <div class="container">
    <% posts.forEach(element => {%>
    <h1><%=element.title%></h1>
    <p>
      <%=element.content.substring(0,100) + '...'%>
      <a href="/posts/<%=element.title%>">Read More</a>
    </p>
    <% }); %>
  </div>
</body>
<%-include('partials/footer') -%>
